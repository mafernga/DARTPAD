void main(){
  
  final wolverine =new Heroe(poder: 'Regeneracion', nombre:'Logan');
  
  print(wolverine);

}

//CLASES, las clases son como un molde, es un standard que a la hora de nombrar las
// clases, estas empiecen con mayuscula


class Heroe{
  
  
  String nombre;
  String poder;
  
  Heroe({String nombre='Sin Nombre', String poder='Sin Poder'}){
    this.nombre=nombre;
    this.poder=poder;
  }

String toString(){
  
  return 'Nombre: ${ this.nombre } - Poder: ${ this.poder }';
}
  
} 
